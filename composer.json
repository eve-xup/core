{
    "name": "xup/core",
    "description": "Core files for xup project",
    "version": "1.0",
    "autoload": {
        "psr-4": {
            "Xup\\Core\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.54",
        "laravelevetools/eve-api": "^1.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/LaravelEveTools/eveapi"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Xup\\Core\\CoreServiceProvider"
            ]
        }
    }

}
